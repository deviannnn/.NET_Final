@model IEnumerable<Pharmacy.Models.QuickLink>

<div class="col-lg-3 mx-auto mb-5 mb-lg-0">
    <h3 class="footer-heading mb-4">Liên kết nhanh</h3>
    <ul class="list-unstyled">
        @foreach (var quicklink in Model)
        {
            <li><a href="@quicklink.link">@quicklink.name</a></li>
        }
    </ul>
</div>